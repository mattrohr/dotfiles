# Move focused window
cmd + ctrl - up : yabai -m window --warp north
cmd + ctrl - right : yabai -m window --warp east
cmd + ctrl - down : yabai -m window --warp south
cmd + ctrl - left : yabai -m window --warp west

# Rotate clockwise
cmd + ctrl - r : yabai -m space --rotate 270

# flip axes
cmd + ctrl - y : yabai -m space --mirror y-axis
cmd + ctrl - x : yabai -m space --mirror x-axis

# Toggle window properties
cmd + ctrl - f : yabai -m window --toggle zoom-fullscreen
cmd + ctrl + shift - f : yabai -m window --toggle float

# Resize
cmd + ctrl + shift - space : yabai -m space --balance
cmd + ctrl + shift - up : yabai -m window --resize bottom:0:-100 ; \
                          yabai -m window --resize top:0:-100
cmd + ctrl + shift - right : yabai -m window --resize right:250:0 ; \
                             yabai -m window --resize left:250:0
cmd + ctrl + shift - down : yabai -m window --resize top:0:100 ; \
                            yabai -m window --resize bottom:0:100
cmd + ctrl + shift - left : yabai -m window --resize left:-250:0 ; \
                            yabai -m window --resize right:-250:0

# Change focus
ctrl + shift - up : yabai -m window --focus north
ctrl + shift - right : yabai -m window --focus east
ctrl + shift - down : yabai -m window --focus south
ctrl + shift - left : yabai -m window --focus west